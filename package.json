{
  "name": "shine",
  "version": "1.0.2",
  "description": "Playwright Automation Framework with BDD",
  "main": "index.js",
  "scripts": {
    "test": "cucumber-js test & npx ts-node ./src/reportGenerator/cucumberReport.ts",
    "test:feature": "cucumber-js -- tests/features/securedcards.feature",
    "test:tags": "cucumber-js --tags @sample",
    "test:report":"npx ts-node ./src/reportGenerator/cucumberReport.ts",
    "test:trace": "npx playwright show-trace",
    "codegen": "npx playwright codegen",
    "clean:reports": "rmdir /s /q reports",
    "clean:nodes": "rmdir /s /q node_modules",
    "clean:mreports": "rm -rf reports",
    "clean:mnodes": "rm -rf node_modules"
  },
  "keywords": [
    "PM",
    "AF"
  ],
  "author": "Prashanth Marappa",
  "license": "ISC",
  "devDependencies": {
    "@axe-core/playwright": "4.10.1",
    "@cucumber/cucumber": "^11.1.0",
    "@cucumber/pretty-formatter": "1.0.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@playwright/test": "^1.49.0",
    "@types/ajv": "^1.0.4",
    "@types/easy-soap-request": "^5.6.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/ibm_db": "2.0.11",
    "@types/jsonpath": "^0.2.0",
    "@types/mssql": "^9.1.5",
    "@types/oracledb": "5.2.3",
    "@types/pdf-parse": "^1.1.1",
    "@types/randomstring": "^1.1.8",
    "@types/string-format": "^2.0.0",
    "ajv": "^8.17.1",
    "cucumber-html-reporter": "^7.2.0",
    "dotenv": "^16.4.5",
    "fetch-to-curl": "0.5.2",
